{
	"name": "df_scd",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_scd1_source",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "ds_scd1_sink",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_scd1_sink",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"rejectedDataLinkedService": {
						"referenceName": "ls_Adls1",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [
				{
					"name": "exists1"
				},
				{
					"name": "alterRow1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Employee as string,",
				"          City as string,",
				"          State as string,",
				"          Tenure as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source(output(",
				"          Employee as string,",
				"          City as string,",
				"          State as string,",
				"          Tenure as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sink",
				"source1, sink exists(source1@Employee == sink@Employee,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"exists1 alterRow(updateIf(true())) ~> alterRow1",
				"alterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Employee as string,",
				"          City as string,",
				"          State as string,",
				"          Tenure as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}